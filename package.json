    "@sanity/default-login": "^1.149.16",
    "@sanity/desk-tool": "^1.149.16",
    "@sanity/vision": "^1.149.16",
    "moment": "^2.24.0",
    "prop-types": "^15.6",
    "react": "^16.2",
    "react-dom": "^16.2",
    "react-icons": "^3.8.0",
    "sanity-plugin-dashboard-widget-netlify": "^1.0.1",
    "sanity-plugin-seo-tools": "^1.0.6",
    "styled-components": "^5.1.1"
